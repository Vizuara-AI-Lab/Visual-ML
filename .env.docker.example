
# Database Configuration
POSTGRES_USER=visualml_user
POSTGRES_PASSWORD=change-this-secure-password
POSTGRES_DB=visualml_prod
DATABASE_URL=postgresql://visualml_user:change-this-secure-password@postgres:5432/visualml_prod

# Redis Configuration
REDIS_PASSWORD=change-this-redis-password

# Application Security
SECRET_KEY=change-this-to-a-secure-random-32-char-minimum-secret-key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_MINUTES=10080

# Email Service (Brevo)
BREVO_API_KEY=your-brevo-api-key-here
BREVO_SENDER_EMAIL=noreply@visualml.com
BREVO_SENDER_NAME=Visual ML

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# AWS S3 (Optional)
USE_S3=false
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
S3_BUCKET=visual-ml-datasets
S3_REGION=us-east-1

# Application Settings
ENVIRONMENT=production
DEBUG=False
FRONTEND_URL=https://yourdomain.com

# Performance
ENABLE_CACHE=True
ENABLE_BACKGROUND_TASKS=True
